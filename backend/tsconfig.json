{
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "verbatimModuleSyntax": true,
    "outDir": "./dist",
    "types":["jest","node"]
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}