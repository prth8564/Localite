{
  "compilerOptions": {
    "target": "esnext",
    "module": "nodenext",
    "jsx": "react-native",
    "strict": true,
    "moduleResolution": "node16",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./src"
  },
  "exclude": [
    "node_modules",
    "build",
    "dist"
  ],
  "extends": "expo/tsconfig.base"
}
